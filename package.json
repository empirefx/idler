{
  "name": "idler",
  "version": "1.0.0",
  "description": "RPG-idler game",
  "source": "src/index.html",
  "targets": {
    "images": {
      "distDir": "./public/images",
      "source": "./src/assets/**"
    },
    "default": {
      "distDir": "./public",
      "source": "./src/index.html"
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build --no-source-maps --no-cache",
    "process-images": "parcel build --target images",
    "test": "vitest"
  },
  "parcel": {
    "plugins": [
      "./image-transform.js"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-react": "^7.26.3",
    "@parcel/config-default": "^2.14.4",
    "@parcel/plugin": "^2.14.4",
    "@parcel/resolver-glob": "^2.14.4",
    "@parcel/transformer-image": "^2.14.4",
    "parcel": "^2.14.4",
    "process": "^0.11.10",
    "sharp": "^0.33.5",
    "vitest": "^3.1.2"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
